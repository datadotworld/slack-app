{
  "name": "slack-app",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "dependencies": {
    "@slack/web-api": "~6.7.2",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "^4.18.1",
    "jade": "~1.11.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "morgan": "~1.9.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "prettysize": "^1.1.0",
    "query-string": "5.1.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-scripts": "^1.1.5",
    "sequelize": "^6.21.1",
    "sequelize-cli": "^6.4.1",
    "serve-favicon": "~2.4.5",
    "url": "^0.11.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "precommit": "./pre-commit.sh",
    "start": "yarn run migration --env development && node --trace-warnings ./server/bin/www",
    "dev": "yarn run heroku-prebuild && yarn run migration && yarn run start",
    "heroku-prebuild": "yarn && yarn run build",
    "build": "react-scripts build",
    "migration": "sequelize-cli db:migrate",
    "test": "jest --notify --forceExit  --clearMocks --detectOpenHandles",
    "test-with-reporter": "jest --forceExit --detectOpenHandles --ci --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^23.3.0",
    "jest-junit": "^5.1.0",
    "sequelize-cli": "^6.4.1",
    "supertest": "^3.1.0"
  }
}
